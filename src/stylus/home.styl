.listItem
  border-style: solid;
  border-width: 60px 60px 50px 60px;
  -moz-border-image: url(/img/item_bg.png) 60 60 50 60 stretch;
  -webkit-border-image: url(/img/item_bg.png) 60 60 50 60 stretch;
  -o-border-image: url(/img/item_bg.png) 60 60 50 60 stretch;
  border-image: url(/img/item_bg.png) 60 60 50 60 fill stretch;

.itemImg
  background-image: url(/img/dishes.png);
  width: 83px;
  height: 80px;
  display: inline-block;

position_left(num)
  if (num%6==0)
    0
  else
    -90*(num%6)px
for x in 0 ... 70
  .item-{x+1}
    background-position: position_left(x) (floor(x/6)*-96)px
